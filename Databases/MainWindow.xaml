<Window x:Class="Databases.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Databases"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="AssetsTag" HorizontalAlignment="Left" Height="333" Margin="41,10,0,0" TextWrapping="Wrap" Text="See Assets
                 " VerticalAlignment="Top" Width="587" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="AssetsAction" Content="Button" HorizontalAlignment="Left" Height="26" Margin="656,10,0,0" VerticalAlignment="Top" Width="72" Click="AssetsAction_Click"/>

    </Grid>
</Window>
